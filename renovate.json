{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["Temurin.kt"],
      "matchStrings": [
        "OpenJDK(?<major>[0-9]+)U-jdk_x64_linux_hotspot_(?<currentValue>[0-9a-z\\._]*)\\.tar\\.gz"
      ],
      "depNameTemplate": "adoptium/temurin{{{major}}}-binaries",
      "datasourceTemplate": "github-release-attachments"
    }
  ]
}
